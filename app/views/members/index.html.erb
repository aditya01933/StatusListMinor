<p id="notice"><%= notice %></p>

<h1>Listing Members</h1>

<table>
  <thead>
    <tr>
      <th>Status</th>
      <th>Ethnicity</th>
      <th>Weight</th>
      <th>Height</th>
      <th>Is veg</th>
      <th>Drink</th>
      <th>Dob</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.status %></td>
        <td><%= member.ethnicity %></td>
        <td><%= member.weight %></td>
        <td><%= member.height %></td>
        <td><%= member.is_veg %></td>
        <td><%= member.drink %></td>
        <td><%= member.dob %></td>
        <td><%= member.image %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

